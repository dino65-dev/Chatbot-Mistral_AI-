{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "env": {
      "API_KEY": "API_KEY",
      "LANGCHAIN_API_KEY": "LANGCHAIN_API_KEY"
    },
    "projectSettings": { "buildCommand": null, "devCommand": null, "framework": "python", "commandForIgnoringBuildStep": "", "installCommand": null, "outputDirectory": null
    }
    }
  